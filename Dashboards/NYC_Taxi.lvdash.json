{
  "datasets": [
    {
      "name": "f37af49f",
      "displayName": "Bronze_nyc",
      "queryLines": [
        "select *,\r\n",
        "CASE WHEN payment_type=1 THEN 'Credit Card' WHEN payment_type=2 THEN 'Cash' WHEN payment_type=3 THEN 'No charge' WHEN payment_type=4 THEN 'Dispute' WHEN payment_type=5 THEN 'Unknown' ELSE 'Voided trip' end as payment_typee from silver_data;"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    },
    {
      "name": "9a89360b",
      "displayName": "Payment_type",
      "queryLines": [
        "select date_a,\r\n",
        "CASE WHEN payment_type=1 THEN 'Credit Card' WHEN payment_type=2 THEN 'Cash' WHEN payment_type=3 THEN 'No charge' WHEN payment_type=4 THEN 'Dispute' WHEN payment_type=5 THEN 'Unknown' ELSE 'Voided trip' end as payment_type\r\n",
        ",trips_total from (select payment_type,count(*) as trips_total,date_format(tpep_pickup_datetime,'yyyy-MM') as date_a from silver_data group by date_a,payment_type order by 3,1);\r\n"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    },
    {
      "name": "95f90999",
      "displayName": "KPI_CHarge_Impact",
      "queryLines": [
        "select sum(total_amount) as total,avg(total_amount) as average_total_amt,\r\n",
        "CASE\r\n",
        "when (mta_tax+improvement_surcharge)>0 then \"Extra\"\r\n",
        "else \"No Extra\" end as extra\r\n",
        "from silver_data group by 3;"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    },
    {
      "name": "1b583680",
      "displayName": "KPI_CHarge_Impact 1",
      "queryLines": [
        "select round(((select sum(\r\n",
        "CASE\r\n",
        "when (mta_tax+improvement_surcharge)>0 then total_amount\r\n",
        "else 0 end) as extra\r\n",
        "from silver_data)*100.0 / (select sum(total_amount) from silver_data)),2) as revenue;"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    },
    {
      "name": "a62ae7ce",
      "displayName": "AVrage_trip_speed",
      "queryLines": [
        "select concat(round(avg(trip_distance/secondss),3),\" Miles/s\") as avg_speed\r\n",
        "from(\r\n",
        "select trip_distance, \r\n",
        "(unix_timestamp(tpep_dropoff_datetime)-unix_timestamp(tpep_pickup_datetime)) as secondss\r\n",
        "from bronze_data)\r\n",
        "where secondss>0;"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    },
    {
      "name": "423ddff3",
      "displayName": "SOlovsGroup",
      "queryLines": [
        "select *,count(*) as total from\r\n",
        "(select \r\n",
        "case when passenger_count = 1 then \"Solo\" \r\n",
        "when passenger_count=2 then \"Dual\"  \r\n",
        "when passenger_count=3 then \"Triple\" \r\n",
        "else \"Group\" end as passenger_type\r\n",
        "from silver_data \r\n",
        "where passenger_count>0)\r\n",
        "group by passenger_type;"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    },
    {
      "name": "f9c08c3e",
      "displayName": "Averagefarepertrip",
      "queryLines": [
        "select concat(\"$. \",round(avg(Fare_amount),2)) as avg_fare from silver_data;"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    },
    {
      "name": "5745a439",
      "displayName": "Vendor_Comparision",
      "queryLines": [
        "select \r\n",
        "case when VendorID = 1 then 'Creative Mobile Technologies' \r\n",
        "when VendorID = 2 then 'VeriFone Inc.' \r\n",
        "else 'Unknown' end as vendor,round(sum(Total_amount),2) as total_revenue\r\n",
        " from silver_data\r\n",
        " group by vendor"
      ],
      "catalog": "nyc_taxi",
      "schema": "default"
    }
  ],
